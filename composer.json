{
    "name": "app/roadrunner-hello",
    "description": "Symfony 8 application with RoadRunner and Temporal",
    "type": "project",
    "require": {
        "php": ">=8.4",
        "baldinof/roadrunner-bundle": "^3.3",
        "symfony/dotenv": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/runtime": "^8.0",
        "symfony/monolog-bundle": "^3.0",
        "temporal/sdk": "2.16.0"
    },
    "require-dev": {
        "buggregator/trap": "^1.15",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^11.0",
        "symfony/browser-kit": "^8.0",
        "symfony/css-selector": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "runtime": {
            "class": "Baldinof\\RoadRunnerBundle\\Runtime\\Runtime"
        }
    }
}
